package part3;
import java.util.*;
public class Remove_dupe {
	static Scanner sc = new Scanner(System.in);
public static void main(String[] args) {
	System.out.println("Enter the String ");
	String s = sc.nextLine();
	
	char[] c = s.toCharArray();
	int l = s.length();
	int count;
	int i,j;
	for(i=0;i<l;i++) {
		count = 0;
		for(j=i+1;j<l;j++) {
			if(c[i]==c[j]) {
				c[i]=0;	
				
			}
				
		}
	
	if(c[i]!=0)
		System.out.print(c[i]);
	}
}
}
